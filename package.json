    "test": "echo \"Error: no test specified\"",
    "build": "babel src/back --out-dir build/back; webpack -p --config config/webpack.release.config.js; git commit -a",
    "build-back": "babel src/back --out-dir build/back; git commit -a",
    "build-front": "webpack -p --config config/webpack.release.config.js; git commit -a",
    "dev-back": "sudo service nginx restart; sudo service babel-nodejs-file.sh stop; sudo service babel-nodejs-file.sh start; sleep 10; sudo service babel-nodejs.sh start",
    "dev-front": "webpack --watch --config config/webpack.dev.config.js",
    "dev-stop": "sudo service nginx stop;sudo service babel-nodejs-file.sh stop; sudo service nodejs-file.sh stop",
    "release": "git pull; sudo service release-nodejs.sh stop; sudo service release-nodejs.sh start",
    "release-file": "git pull; sudo service nginx-release restart; sudo service release-nodejs-file.sh stop; sudo service release-nodejs-file.sh start",
    "release-stop": "sudo service nginx-release stop; sudo service release-nodejs-file.sh stop; sudo service release-nodejs.sh stop"
  },
